# =======================================================
# [TradeNex Environment Variables Sample]
# 이 파일을 복사하여 .env 로 이름을 변경한 뒤, 실제 키 값을 입력하세요.
# =======================================================

# [통합 공통 키]
# OpenAI GPT 모델 사용을 위한 API 키 (필수)
OPENAI_API_KEY=sk-proj-YOUR_OPENAI_API_KEY_HERE

# ------------------------------------------------------------------------------------------

# [영업팀 - Sales Team]
# 환율 정보 조회를 위한 인증 키
EXCHANGE_RATE_KEY=YOUR_EXCHANGE_RATE_KEY_HERE

# 환율 API 엔드포인트 또는 추가 인증 키
RATE_EXCHANGE_API=YOUR_RATE_EXCHANGE_API_URL_OR_KEY

# 글로벌 뉴스 검색을 위한 API 키
News_API=YOUR_NEWS_API_KEY_HERE

# ------------------------------------------------------------------------------------------

# [물류팀 - Logistics Team]
# HS Code 조회를 위한 API
HS_SEARCH_API=YOUR_HS_SEARCH_API_KEY_HERE

# 기본 운임 조회 API
RATE_BASIC_API=YOUR_BASIC_RATE_API_KEY_HERE

# 품목 확인 및 검증 API
ITEM_CONFIRM_API=YOUR_ITEM_CONFIRM_API_KEY_HERE

# 관세청 통관 정보 조회 API (Customs)
CUSTUM_API=YOUR_CUSTOMS_API_KEY_HERE

# 운송 거리 계산 API (Google Maps 등)
DISTANCE_API=YOUR_DISTANCE_API_KEY_HERE

# ------------------------------------------------------------------------------------------

# [구매팀 - Purchasing Team]
# Tavily 검색 엔진 API (시장 조사용)
TAVILY_API_KEY=tvly-YOUR_TAVILY_API_KEY_HERE

# 제품 상태/정보 조회 API
PRODUCT_CON_API_KEY=YOUR_PRODUCT_CONDITION_API_KEY_HERE

# 식품안전나라 HACCP 인증 정보 조회 API
HACCP_API_KEY=YOUR_HACCP_API_KEY_HERE